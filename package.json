{
  "name": "wc",
  "version": "0.3.14",
  "description": "Wellness Challenge",
  "scripts": {
    "start": "node server.js",
    "clean": "rimraf lib dist",
    "lint": "eslint src test",
    "test": "NODE_ENV=production mocha --compilers js:babel/register --recursive",
    "test:watch": "npm test -- --watch",
    "check": "npm run lint && npm run test",
    "build": "webpack src/index.js --config webpack.config.production.js",
    "preversion": "npm run clean && npm run check",
    "version": "npm run build",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nematode/wc.git"
  },
  "keywords": [
    "wellness",
    "challenge",
    "social"
  ],
  "author": "Trevor Farlow <npm@trevdiggy.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nematode/wc/issues"
  },
  "homepage": "https://github.com/nematode/wc",
  "dependencies": {
    "immutable": "^3.8.1",
    "lodash": "^4.11.1",
    "material-ui": "0.15.0-alpha.1",
    "moment": "^2.13.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.3.0",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.3",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^2.8.0",
    "eslint-plugin-import": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^1.0.2",
    "eslint-plugin-react": "^5.0.1",
    "express": "^4.13.4",
    "jsdom": "^8.4.0",
    "material-design-icons": "^2.2.3",
    "mocha": "^2.4.5",
    "mocha-jsdom": "^1.1.0",
    "path": "^0.12.7",
    "proxyquire": "^1.7.4",
    "react-addons-test-utils": "^15.0.1",
