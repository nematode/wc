{
  "name": "wc",
  "version": "0.3.14",
  "description": "Wellness Challenge",
  "main": "lib/index.js",
  "scripts": {
    "start": "node server.js",
    "clean": "rimraf lib dist coverage",
    "lint": "eslint src test",
    "test": "NODE_ENV=production mocha --compilers js:babel/register --recursive",
    "test:watch": "npm test -- --watch",
    "test:cov": "babel-node $(npm bin)/isparta cover $(npm bin)/_mocha -- --recursive",
    "check": "npm run lint && npm run test  && flow check ",
    "build": "webpack src/index.js --config webpack.config.production.js",
    "preversion": "npm run clean && npm run check",
    "version": "npm run build",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nematode/wc.git"
  },
  "keywords": [
    "wellness",
    "challenge",
    "social"
  ],
  "author": "Trevor Farlow <npm@trevdiggy.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nematode/wc/issues"
  },
  "homepage": "https://github.com/nematode/wc",
  "dependencies": {
    "immutable": "^3.7.5",
    "lodash": "^3.10.1",
    "material-ui": "^0.11",
    "moment": "^2.10.6",
    "react": "^0.14.0-rc1",
    "react-dom": "^0.14.0-rc1",
    "react-redux": "^2.1.2",
    "react-router": "^1.0.0-rc1",
    "react-tap-event-plugin": ">=0.1.3",
    "redux": "^3.0.0",
    "redux-logger": "^1.0.8",
    "redux-promise": "^0.5.0",
    "redux-thunk": "^1.0.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.24",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.0.5",
    "chai": "^3.2.0",
    "eslint": "^1.4.3",
    "eslint-config-airbnb": "^0.0.8",
    "eslint-plugin-react": "^3.4.1",
    "express": "^4.13.3",
    "history": "^1.9.1",
    "isparta": "^3.0.3",
    "jsdom": "^6.1.0",
    "karma": "^0.13.9",
    "karma-babel-preprocessor": "^5.2.2",
    "karma-chrome-launcher": "^0.2.0",
    "karma-coverage": "^0.5.2",
    "karma-firefox-launcher": "^0.1.6",
    "karma-jasmine": "^0.3.6",
    "karma-requirejs": "^0.2.2",
    "karma-sinon": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "material-design-icons": "^2.0.0",
    "mocha": "*",
    "mocha-jsdom": "^1.0.0",
    "path": "^0.11.14",
    "proxyquire": "^1.7.2",
    "react-addons-test-utils": "^0.14.0-rc1",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.0",
    "redbox-react": "^1.0.1",
    "redux-devtools": "^2.1.2",
    "requirejs": "^2.1.20",
    "rimraf": "^2.4.3",
    "sinon": "^1.16.1",
    "webpack": "^1.12.0",
